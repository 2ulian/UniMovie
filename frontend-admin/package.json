{
  "name": "netflix-admin",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"nodemon server.js\" \"npm run watch:css\"",
    "watch:css": "tailwindcss -i ./public/css/input.css -o ./public/css/output.css --\nwatch",
    "build:css": "tailwindcss -i ./public/css/input.css -o ./public/css/output.css --min-\nify",
    "start": "node server.js"
  }
}
